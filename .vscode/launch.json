{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "streamsync - edit",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/streamsync",
            "args": [
                "--enable-remote-edit",
                "edit",
                "src/app"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
